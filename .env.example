# ee-cp
CP_BIND_ADDR=0.0.0.0:8080
CP_DATABASE_URL=sqlite://easyenclave.db
CP_DOMAIN=easyenclave.com
CF_ACCOUNT_ID=
CF_API_TOKEN=
CF_ZONE_ID=
ITA_JWKS_URL=https://api.trustauthority.intel.com/.well-known/jwks.json
ITA_APPRAISAL_URL=https://api.trustauthority.intel.com/appraisal/v2/attest
ITA_API_KEY=
CP_ALLOW_INSECURE_TEST_ATTESTATION=false
GITHUB_OIDC_JWKS_URL=https://token.actions.githubusercontent.com/.well-known/jwks
GITHUB_OIDC_ISSUER=https://token.actions.githubusercontent.com
GITHUB_OIDC_AUDIENCE=
CP_ALLOW_INSECURE_TEST_OIDC=false

# ee-agent
AGENT_MODE=agent
AGENT_VM_NAME=ee-agent-1
AGENT_OWNER=github:org/example
AGENT_NODE_SIZE=c3-standard-4
AGENT_DATACENTER=gcp:us-central1-a
AGENT_CP_URL=http://127.0.0.1:8080
AGENT_BIND_ADDR=0.0.0.0:9000
AGENT_HEARTBEAT_SECONDS=60
AGENT_ALLOW_MOCK_QUOTE=true
# Optional: path to a prebuilt raw quote blob
AGENT_QUOTE_PATH=
# Optional: shell command that prints raw quote bytes to stdout (nonce in $EE_NONCE_HEX)
AGENT_TDX_QUOTE_CMD=
# Optional: configfs-tsm paths used when AGENT_QUOTE_PATH and AGENT_TDX_QUOTE_CMD are unset
AGENT_TSM_REPORT_DATA_PATH=/sys/kernel/config/tsm/report/reportdata
AGENT_TSM_QUOTE_PATH=/sys/kernel/config/tsm/report/quote

# ee-launcher
LAUNCHER_QEMU_BIN=qemu-system-x86_64
LAUNCHER_DEFAULT_MEMORY_MB=8192
LAUNCHER_DEFAULT_VCPUS=4
